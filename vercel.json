{
    "version": 2,
    "routes": [
      {
        "handle": "src/api/resultado/megasena.js",
        "methods": ["GET"],
        "src": "/api/resultado/megasena"
      },
      {
        "handle": "src/api/resultado/duplasena.js",
        "methods": ["GET"],
        "src": "/api/resultado/duplasena"
      },
      {
        "src": "/api/resultado/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" },
          { "key": "Access-Control-Allow-Methods", "value": "GET" }
        ],
        "continue": true
      },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }  