{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/resultado/megasena",
            "methods": ["GET"],
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET",
                "Access-Control-Allow-Headers": "Content-Type",
                "Access-Control-Allow-Max-Age": "86400"
            },
            "dest": "index.js"
        },
        {
            "src": "/api/resultado/megasena/(.*)",
            "methods": ["GET"],
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET",
                "Access-Control-Allow-Headers": "Content-Type",
                "Access-Control-Allow-Max-Age": "86400"
            },
            "dest": "index.js"
        },
        {
            "src": "/api/resultado/duplasena",
            "methods": ["GET"],
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET",
                "Access-Control-Allow-Headers": "Content-Type",
                "Access-Control-Allow-Max-Age": "86400"
            },
            "dest": "index.js"
        },
        {
            "src": "/api/resultado/duplasena/(.*)",
            "methods": ["GET"],
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET",
                "Access-Control-Allow-Headers": "Content-Type",
                "Access-Control-Allow-Max-Age": "86400"
            },
            "dest": "index.js"
        }
    ]
}